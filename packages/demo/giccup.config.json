{
  "$schema": "./node_modules/@gcf-tools/cli/giccup.config.schema.json",
  "projects": {
    "gcp-nodejs-prod": {
      "environmentFile": "./environments/production.env.yml",
      "functions": {
        "receiveWebhook": {
          "runtime": "nodejs10"
        },
        "receivePubSub": {
          "runtime": "nodejs10",
          "trigger": {
            "topic": "demo"
          }
        }
      }
    },

    "gcp-nodejs-dev": {
      "environmentFile": "./environments/development.env.yml",
      "functions": {
        "receiveWebhook": {
          "runtime": "nodejs10"
        },
        "receivePubSub": {
          "runtime": "nodejs10",
          "trigger": {
            "topic": "demo"
          }
        }
      }
    }
  }
}
